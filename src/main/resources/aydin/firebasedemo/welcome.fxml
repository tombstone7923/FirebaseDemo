<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="219.0" prefWidth="375.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aydin.firebasedemo.welcomeController">
    <children>
        <Label text="Welcome" />
      <TextField fx:id="emailTextField" promptText="Email" />
      <TextField fx:id="phoneNumberTextField" promptText="Phone Number" />
      <TextField fx:id="usernameTextField" promptText="Username" />
      <TextField fx:id="passwordTextField" promptText="Password" />
      <ButtonBar prefHeight="40.0" prefWidth="200.0">
        <buttons>
            <Button fx:id="registerButton" mnemonicParsing="false" onAction="#registerButtonClicked" prefHeight="30.0" prefWidth="80.0" text="Register" />
            <Button mnemonicParsing="false" text="Login" />
        </buttons>
      </ButtonBar>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
